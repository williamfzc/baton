{
  "name": "baton",
  "version": "0.1.0",
  "description": "Connect IM with local ACP agents",
  "type": "module",
  "main": "src/cli.ts",
  "bin": {
    "baton": "src/cli.ts"
  },
  "scripts": {
    "start": "bun src/cli.ts",
    "test": "bun test",
    "dev": "bun --watch src/cli.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun build --compile src/cli.ts --outfile baton"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.14.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}